{
  "name": "reindeerfinder",
  "version": "1.0.0",
  "homepage": "https://reinmerker.no",
  "description": "Find the owner of a reindeer based on ear marks",
  "keywords": [
    "reindeer",
    "norway"
  ],
  "scripts": {
    "build:clean": "rm -rf dist/",
    "build:html": "cp -r src/html/* dist/",
    "build:css": "cp -r src/css/* dist/",
    "build:js": "NODE_ENV=production browserify src/js/index.js | uglifyjs > dist/rein.js",
    "watch": "budo src/js/index.js:rein.js -d src/html/ -d src/css/ -P -l -o",
    "prebuild": "npm run build:clean",
    "build": "mkdir dist && npm run build:html && npm run build:css && npm run build:js"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "budo": "^9.2.1",
    "bulkify": "^1.4.2",
    "choo-log": "^3.0.0",
    "envify": "^4.0.0",
    "uglifyify": "^3.0.4",
    "uglifyjs": "^2.4.10",
    "unassertify": "^2.0.3",
    "watchify": "^3.8.0",
    "yo-yoify": "^3.5.0"
  },
  "repository": {
    "url": "git://github.com/ruudud/ReindeerFinder.git",
    "type": "git"
  },
  "author": {
    "name": "PÃ¥l Ruud",
    "email": "ruudud@gmail.com",
    "url": "http://github.com/ruudud"
  },
  "license": "AGPL-3.0",
  "browserify": {
    "plugin": [],
    "global": [
      "envify",
      "uglifyify"
    ],
    "transform": [
      "unassertify",
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ],
      "yo-yoify"
    ]
  },
  "dependencies": {
    "choo": "^4.0.0"
  }
}
